<Project Sdk="Microsoft.NET.Sdk">


	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<NoWarn>1701;1702;1591</NoWarn>
		<DocumentationFile>MyBlog.Server.Application.xml</DocumentationFile>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>


	<ItemGroup>
	  <Compile Remove="System\**" />
	  <EmbeddedResource Remove="System\**" />
	  <None Remove="System\**" />
	</ItemGroup>



	<ItemGroup>
		<None Remove="applicationsettings.json" />
		<None Remove="MyBlog.Server.Application.xml" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="applicationsettings.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\MyBlog.Server.Core\MyBlog.Server.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Client\" />
	</ItemGroup>

</Project>
